    "lint:css": "yarn stylelint:check",
    "prettier:check": "prettier -l .",
    "prettier:fix": "prettier -w .",
    "eslint:check": "eslint --report-unused-disable-directives .",
    "eslint:ci:check": "eslint-output --report-unused-disable-directives .",
